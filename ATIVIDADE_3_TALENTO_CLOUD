print("\n--------- Ordem decrescente----------") 
andar = 20
while andar >= 1:
    if andar != 13:
        print(andar)
    andar -= 1
print("\n--------------- FIM -----------------")


print("\n------------RANGER-------------------")
print("\n-------------------------------------")
for andar in range(20, 0, -1):
    if andar != 13:
        print(andar)
print("\n-------------------------------------")       
print("\n--------------- FIM -----------------")